<div class="container" id="resourceDetailDiv">
    <ol class="breadcrumb">
        <li><a href="/admin">Home</a></li>
        <li>iconManage</li>
    </ol>
    <div class="collection-detail">
        <h4 class="block-collection-title">
            <span>所有图标资源</span>
        </h4>
        <div class="btn-group">
            <a class="btn btn-primary" href="#editIcon" role="button">添加</a>
        </div>
        <div class="row">
            <ul>
                <div class="row">
                <% iconList.forEach(function(value){ %>
                    <% if(value.fileType == 0 ){ %>
                    <div class="col-xs-6 col-md-3">
                        <div class="caption">
                            <h3><%- value.name %></h3>
                            <img src=<%- value.svgfile %> alt="">
                            <p>tags : <%- value.tags %></p>
                            <p><a href="#editIcon/<%- value.id %>" class="btn btn-primary" role="button">edit</a> <a href="#" class="btn btn-danger" role="button">del</a></p>
                        </div>
                    </div>
                    <% }else if(value.fileType == 1 ){ %>
                    <div class="col-xs-6 col-md-3">
                        <a href="#editIcon/<%- value.id %>" class="thumbnail">
                            <img src=<%- value.pngfile %> alt="">
                        </a>
                    </div>
                    <% }else if(value.fileType == 2 ){ %>
                    <div class="col-xs-6 col-md-3">
                        <a href="#editIcon/<%- value.id %>" class="thumbnail">
                            <img src=<%- value.psdfile %> alt="">
                        </a>
                    </div>
                    <% }else if(value.fileType == 3 ){ %>
                    <div class="col-xs-6 col-md-3">
                        <a href="#editIcon/<%- value.id %>" class="thumbnail">
                            <img src=<%- value.zipfile %> alt="">
                        </a>
                    </div>
                    <% } %>
                <%})%>
                </div>
            </ul>
        </div>
    </div>
</div>