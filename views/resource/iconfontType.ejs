<% include ../common/head.ejs %>
<div class="h-bg">
    <div class="main-top">
        <% include ../common/header.ejs %>
    </div>
    <% include ../resource/submenu.ejs %>
    <div class="h-section minh-485">
        <div class="h-container">
            <!--内容部分-->
            <div class="collection-detail">
                <h4 class="block-collection-title">
                    <span><%= collectionName %></span>
                    <i class="total-num">20</i>
                </h4>
                <ul class="icons-container clearfix">
                    <% iconList.forEach(function(value){ %>
                    <li><img src=<%- value.url %> alt=""></li>
                    <% }) %>
                </ul>
            </div>
        </div>
    </div>
    <% include ../common/footer.ejs %>
</div>
<% include ../common/foot.ejs %>
