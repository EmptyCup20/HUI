<% include ../common/head.ejs %>
<div class="main-wrapper h-bg">
    <div class="main-top">
        <% include ../common/header.ejs %>
    </div>
    <div class="h-section minh-485">
        <div class="h-container">
            <!--内容部分-->
            <div class="collection-detail">
                <h4 class="block-collection-title">
                    <span>所有图标资源</span>
                    <i class="total-num">20</i>
                </h4>
                <ul class="icons-container clearfix">
                    <% iconList.forEach(function(value){ %>
                        <% if(value.fileType == 0 ){ %>
                            <li><a href="/resource/detail/<%- value.id %>"><img src=<%- value.svgfile %> alt=""></a></li>
                        <% }else if(value.fileType == 1 ){ %>
                            <li><a href="/resource/detail/<%- value.id %>"><img src=<%- value.pngfile %> alt=""></a></li>
                        <% }else if(value.fileType == 2 ){ %>
                            <li><a href="/resource/detail/<%- value.id %>"><img src=<%- value.psdfile %> alt=""></a></li>
                        <% }else if(value.fileType == 3 ){ %>
                            <li><a href="/resource/detail/<%- value.id %>"><img src=<%- value.zipfile %> alt=""></a></li>
                        <% } %>
                    <%})%>
                </ul>
            </div>
        </div>
    </div>
    <% include ../common/footer.ejs %>
</div>
<% include ../common/foot.ejs %>